{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Main File",
      "type": "shell",
      "command": "${workspaceFolder}/src/py/myenv/bin/python3",
      "args": ["${workspaceFolder}/src/py/main.py"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      }
    }
  ]
}
